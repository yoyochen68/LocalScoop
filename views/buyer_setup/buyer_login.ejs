<body class="indexpage">

    <header class="shop-setup-header">
        <!--Back button-->
        <a onclick="history.back()"><span class="material-icons back-icon">arrow_back</span></a>
    
    </header>


<div class="form-box center">
    <form class="p-3"  action="/buyer_setup/buyer_login" method="POST">
    <!-- <form class="p-3"  action="/seller_landing/seller_landing/:id" method="POST"> -->
        <h5>Enter contact Info</h5>

        <div class=" m-3">
            <label for="buyer_email">Email Address</label>
            <input type="email" class="form-control" id="buyer_email" name="buyer_email">
        </div>
        <br>
        <div class=" m-3">
            <h5>Create Password</h5>
            <label for="buyer_password">Password</label>
            <input type="password" class="form-control" id="buyer_password" name="buyer_password">
        <small><a href=""> Forgot password?</a></small>
        </div>
        <button type="submit" class="yellow-bn m-3" >Login </button>
    </form>
</div>
